<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/orange</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/black</item>
        <item name="android:windowBackground">@color/white</item>

    </style>

    <style name="ImagePreviewerTheme" >
        <item name="android:windowIsFloating">false</item>
    </style>

    <style name="searchView">
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginRight">@dimen/searchMarginRL</item>
        <item name="android:layout_marginLeft">@dimen/searchMarginRL</item>
        <item name="android:layout_marginBottom">@dimen/searchMarginBM</item>
    </style>





</resources>
